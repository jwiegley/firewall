<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>org.snort</string>
	<key>OnDemand</key>
	<false/>
	<key>Program</key>
	<string>/usr/local/bin/snort_inline</string>
	<key>ProgramArguments</key>
	<array>
		<string>sort_inline</string>
		<string>-l</string>
		<string>/usr/local/var/log/snort</string>
		<string>-J</string>
		<string>8000</string>
		<string>-b</string>
		<string>-c</string>
		<string>/usr/local/etc/snort_inline.conf</string>
	</array>
</dict>
</plist>
